{
  "name": "@charno/linked-data-toolkit",
  "version": "2.0.0",
  "description": "A modern TypeScript toolkit for SPARQL lookups against various authorities and linked data sources",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "linked-data-toolkit": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "linked-data",
    "sparql",
    "semantic-web",
    "dbpedia",
    "geonames",
    "library-of-congress",
    "ordnance-survey",
    "rdf",
    "typescript"
  ],
  "author": "Michael Charno <michael@charno.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcharno/linked-data-toolkit.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "MIGRATION.md"
  ]
}
